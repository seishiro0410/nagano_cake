 <div class="container">
          <p id="notice"></p>
          <h2>ジャンル編集</h2>

          <%= form_with model: @genre, url: admin_genre_path do |f| %>

              <div class="field">
                <%= f.label :name, "ジャンル名" %>
                <%= f.text_field :name, class: 'form-control' %>
                <%= f.submit '変更を保存', class: "btn btn-success" %>
              </div>
          <% end %>
        </div>